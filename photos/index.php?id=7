	var imagearray = new Array();
	var imagenames = new Array();
	var imagewidths = new Array();
	var imageheights = new Array();	
	var imagetopoffsets = new Array();
	var imageleftoffsets = new Array();
	var imageurls = new Array();	
	
	function rand(minval, maxval)
	{
		return Math.floor(Math.random()*maxval + minval);
	}
	
	
	function UpdatePic()
	{
		if (img = document.getElementById('pic')){
			img.src = imagearray[index].src;
			img.innerHTML = imagearray[index];
			img.style.paddingTop = imagetopoffsets[index] + "px";
			img.style.paddingLeft = imageleftoffsets[index] + "px";	
			img.style.height = (height-imagetopoffsets[index])+"px";
			img.style.width = (width-imageleftoffsets[index])+"px";
			
		}
		
		if (countdiv = document.getElementById('top')){
			var newtext = "" + (index+1) + " of " + count + "";
			countdiv.innerHTML = newtext;
		}
		
		if (captiondiv = document.getElementById('bottom')){
			captiondiv.innerHTML = imagenames[index];
		}
	}		
		
	function GetRandomPic()
	{
		index = rand(0, count);
		//alert("randpic");
		UpdatePic();
	}
	
	function NextPic()
	{
		index++;
		if (index >= count)
			index = 0;
		//alert("nextpic");			
		UpdatePic();			
	}
	
	function PrevPic()
	{
		index--;
		if (index < 0)
			index = count-1;
		//alert("prevpic");			
		UpdatePic();		
	}
	
	function FullSizedPic()
	{
		PopupWindow("http://www.stanleysubmarines.com/animal_life/images/" + imageurls[index], imagewidths[index], imageheights[index]);
	}
	
	function EmailPic()
	{
		url = "../porthole/emailfriend.php?picurl=";
		url += "http://www.stanleysubmarines.com/animal_life/images/" + imageurls[index];
		url += "&caption=";
		url += imagenames[index];
		PopupWindow(url, 350, 320);
	}	
	
